{
  "expo": {
    "name": "Listen Along",
    "slug": "listen-along-mobile",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "scheme": "listenalong",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.1.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2B59A2"
    },
    "updates": {
      "fallbackToCacheTimeout": 30000
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "fm.listenalong"
    },
    "android": {
      "package": "fm.listenalong"
    }
  }
}
